                                            <i:EventTrigger EventName="MouseUp">
                                                <i:InvokeCommandAction Command="{Binding ForwardUpCommand}" CommandParameter="btn_ForwardZ2"/>
                                            </i:EventTrigger>
                                            <i:EventTrigger EventName="MouseDown">
                                                <i:InvokeCommandAction Command="{Binding ForwardDownCommand}" CommandParameter="btn_ForwardZ2"/>
                                            </i:EventTrigger>
